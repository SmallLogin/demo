    "# encoding: UTF-8\n",
    "import glob as gb\n",
    "import cv2\n",
    "\n",
    "#Returns a list of all folders with participant numbers\n",
    "img_path = gb.glob(\"D:\\pic\\\\*.jpg\") \n",
    "for path in img_path:\n",
    "    img  = cv2.imread(path) \n",
    "    cv2.imshow('img',img)\n",
    "    cv2.waitKey(1000)\n"
